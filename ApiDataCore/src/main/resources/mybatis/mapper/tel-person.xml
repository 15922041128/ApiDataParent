<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.pbccrc.api.core.mapper.TelPersonMapper">
	
	<select id="getInnerID" parameterType="java.lang.String" resultType="java.util.HashMap">
		select * from tel_person where telNum = #{telNum} 
	</select>
	
	<select id="getTelPerson" parameterType="java.lang.String" resultType="java.util.HashMap">
		select * from tel_person where innerID = #{innerID} 
	</select>
	
</mapper>