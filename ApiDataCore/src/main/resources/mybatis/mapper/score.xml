<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.pbccrc.api.core.mapper.ScoreMapper">
	
	<select id="getScore" parameterType="java.lang.String" resultType="java.util.HashMap">
		select score from score where innerID = #{innerID}
	</select>
	
	<select id="getScore201705" parameterType="java.lang.String" resultType="java.util.HashMap">
		select score from SCORE201705 where innerID = #{innerID}
	</select>
	
</mapper>