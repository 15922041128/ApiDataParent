<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.pbccrc.api.core.mapper.ScoreTriMapper">
	
	<select id="getScore" parameterType="java.lang.String" resultType="java.util.HashMap">
		select
	      innerid,
	      ely,
	      ul,
	      ln,
	      som,
	      ton,
	      hln,
	      cln,
	      pbln,
	      lon,
	      san,
	      eiy,
	      cnn,
	      ott,   
	      omt,
	      unm,
	      gnm,
	      con,
	      score,
	      fscore,
	      sscore,
	      99 as ar6,
	      99 as ab6
		from score_tri where innerid = #{innerid}
	</select>
	
</mapper>