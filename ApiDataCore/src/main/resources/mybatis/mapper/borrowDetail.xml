<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.pbccrc.api.core.mapper.BorrowDetailMapper">

	<select id="getBorrowDetail" parameterType="java.util.HashMap" resultType="java.util.HashMap">
		select * from (
			select * from borrow_detail where id_card = #{idCard} and real_name = #{realName}
		) where rownum = 1
	</select>
	
    
    
	
</mapper>