<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.pbccrc.api.core.mapper.GradeMapper">
	
	<select id="getGrade" parameterType="java.util.HashMap" resultType="java.util.HashMap">
		select * from grade where userID = #{userID} and grade = #{grade}
	</select>
	
</mapper>